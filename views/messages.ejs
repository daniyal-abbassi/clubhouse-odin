<ul>
    <%if(messages&&messages.length>0) {%>
        <% messages.forEach(message=>{%>
            <li><h3><%=message.text%></h3><small><%=message.created_at%></small></li>
        <%})%>
    <%} else {%>
        <p>No Posts Yet!</p>
    <%}%>
</ul>

<button><a href="/messages/new-message">New Message</a></button>